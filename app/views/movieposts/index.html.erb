<p id="notice"><%= notice %></p>

<h1>Listing Movieposts</h1>

<table>
  <thead>
    <tr>
      <th>Content</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @movieposts.each do |moviepost| %>
      <tr>
        <td><%= moviepost.content %></td>
        <td><%= link_to 'Show', moviepost %></td>
        <td><%= link_to 'Edit', edit_moviepost_path(moviepost) %></td>
        <td><%= link_to 'Destroy', moviepost, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Moviepost', new_moviepost_path %>
